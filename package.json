{
  "name": "react-img-editor-en",
  "version": "0.1.4",
  "description": "Image Annotation Tool for React",
  "keywords": [
    "react-img-editor-en",
    "image",
    "editor",
    "react",
    "konva",
    "javascript",
    "annotation"
  ],
  "main": "lib/index",
  "module": "es/index",
  "scripts": {
    "start": "rc-tools run server",
